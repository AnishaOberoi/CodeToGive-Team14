<!DOCTYPE html>
<html>

<head>
    <title>Survey Report</title>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,700" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css">
    <link rel="stylesheet"
        href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.1/css/all.css"
        integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" href="navbar.css">
    <link rel="stylesheet" href="footer.css">

    <link rel="stylesheet" href="report.css">
</head>

<body>
    <div id="wrapper">
        <div id="content">
            <nav class="navbar navbar-expand-lg nav_top ">

                <div class="container-fluid">
                    <a class="navbar-brand" href="#" id="logo">
                        <img src="https://breadsbangalore.org/wp-content/themes/Breads_/images/logo-wide.png" alt=""
                            max-width="400" max-height="70" class="d-inline-block align-text-top">
                    </a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                        data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                            <li class="nav-item">
                                <a class="nav-link active" aria-current="page" href="/">Home</a>
                            </li>
                            <!-- <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="./main.ejs">About Us</a>
                    </li> -->
                            <li class="nav-item">
                                <a class="nav-link active" aria-current="page" target="_blank"
                                    href="https://breadsbangalore.org/contact">Contact Us</a>
                            </li>


                        </ul>
                        <div class="d-flex">

                        </div>


                    </div>
                </div>
            </nav>


            <section class="all-report">
                <h1>All Users Report</h1>
                <div class="allexport-button">
                    <button id="allexportButton">Export to Excel</button>
                </div>

                <table id="allTable" style="display: none;">
                    <thead>
                        <tr>
                            <th>Sr. No.</th>
                            <th>Name</th>
                            <th>Pincode</th>
                            <th>District</th>
                            <th>Age</th>
                            <th>Know what addiction is</th>
                            <th>Type of Addiction</th>
                            <th>Feelings experienced</th>
                            <th>Dependency on substance</th>
                            <th>How likely you are accepting a substance</th>
                            <th>Feel stressed</th>
                            <th>Experienced difficulties in controlling yourself?</th>
                            <th>Do you know any people in these substances?</th>
                            <th>Willing for Counselling</th>
                            <th>Willing to Volunteer</th>
                            <th>Contact</th>
                            <th>Prefered Timings</th>
                            <th>Date of survey</th>




                        </tr>
                    </thead>
                    <tbody>

                        <% i=1; %>
                            <%all.forEach(function (survey) { %>
                                <% var keys=Object.keys(survey);%>
                                    <% var
                                        heads=["name","pincode","dis","age","know","add","feel","dependence","scale","stressed","control","others","vol","cou","contact","timings","date"];
                                        %>
                                        <% var val=Object.values(survey);%>
                                            <!-- <%= keys%>
                                                <%= val%>
                                                    <%= heads%> -->
                                            <tr>
                                                <td>
                                                    <%=i%>
                                                        <%i=i+1%>


                                                </td>
                                                <%heads.forEach(function (head) {%>
                                                    <%if(keys.indexOf(head)==-1){%>
                                                        <td>
                                                            <%=""%>
                                                        </td>
                                                        <%}else{%>
                                                            <td>
                                                                <%=survey[head]%>
                                                            </td>
                                                            <%}%>

                                                                <% });%>
                                            </tr>

                                            <%});%>

                    </tbody>
                </table>


            </section>

            <section class="volunteer-report">
                <h1>Volunteer</h1>
                <div class="volunteerexport-button">
                    <button id="volunteerexportButton">Export to Excel</button>
                </div>

                <table id="volunteerTable">
                    <thead>
                        <tr>
                            <th>Sr. No.</th>
                            <th>Name</th>
                            <th>Contact</th>
                            <th>Preffered Timings</th>
                            <!-- Add more columns if needed -->
                        </tr>
                    </thead>
                    <tbody>
                        <% i=1; %>
                            <%volunteer.forEach(function (survey) { %>

                                <tr>
                                    <td>
                                        <%=i%>
                                            <%i=i+1%>
                                    </td>
                                    <%Object.values(survey).forEach(function (value) {%>
                                        <td>
                                            <%=value%>
                                        </td>
                                        <% });%>
                                </tr>

                                <%});%>
                                    <!-- Add rows with survey data dynamically using JavaScript -->

                    </tbody>
                </table>


            </section>




            <section class="counsel-report">
                <h1>Counselling</h1>
                <div class="counselexport-button">
                    <button id="counselexportButton">Export to Excel</button>
                </div>

                <table id="counselTable">
                    <thead>
                        <tr>
                            <th>Sr. No.</th>
                            <th>Name</th>
                            <th>Contact</th>
                            <th>Preffered Timings</th>
                            <!-- Add more columns if needed -->
                        </tr>
                    </thead>
                    <tbody>
                        <% i=1; %>
                            <%counsel.forEach(function (survey) { %>

                                <tr>
                                    <td>
                                        <%=i%>
                                            <%i=i+1%>


                                    </td>
                                    <%Object.values(survey).forEach(function (value) {%>
                                        <td>
                                            <%=value%>
                                        </td>
                                        <% });%>
                                </tr>

                                <%});%>
                                    <!-- Add rows with survey data dynamically using JavaScript -->

                    </tbody>
                </table>


            </section>







        </div>
    </div>
</body>
